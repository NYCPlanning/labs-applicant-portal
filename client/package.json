{
  "name": "client",
  "version": "0.0.0",
  "private": true,
  "description": "NYC Planning Labs Applicant Portal Ember web app.",
  "repository": "https://github.com/NYCPlanning/labs-applicant-portal",
  "license": "MIT",
  "author": "NYC Planning Labs",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "prepare": "cd .. && husky install .husky",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "start:dev": "HOST=https://local.planninglabs.nyc:3000 ember serve --host local.planninglabs.nyc --ssl",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test"
  },
