<Ui::Breadcrumbs as |Crumb|>
  <Crumb @text="My Projects" @route='projects' />
  <Crumb
    @text={{@model.package.project.dcpProjectname}}
    @route='project'
    @model={{@model.package.project.id}}
  />
  <Crumb @text={{optionset 'package' 'dcpPackagetype' 'label' @model.package.dcpPackagetype}}
    @disabled={{true}}
  />
  <Crumb @text="Edit" @current={{true}} />
</Ui::Breadcrumbs>

<Packages::LanduseForm::Edit
  @package={{@model.package}}
  @accounts={{@model.accounts}}
  @zoningResolutions={{@model.zoningResolutions}}
/>

{{outlet}}
