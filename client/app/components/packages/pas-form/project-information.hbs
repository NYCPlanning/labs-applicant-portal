{{#let @form as |Form|}}
  <Form.Section @title="Project Information">
    <Ui::Question @required={{true}} as |Q|>
      <Q.Label>
        Project Name
      </Q.Label>

      <Form.Field
        @attribute="dcpRevisedprojectname"
        id={{Q.questionId}}
      />
    </Ui::Question>
  </Form.Section>
{{/let}}
