      <p>
        <strong>
          Description of geography
        </strong>
        <br>
        {{@package.pasForm.dcpDescriptionofprojectareageography}}
      </p>
    </section>

    <section class="form-section" id="#proposed-land-use-actions">
      <h3>Proposed Land Use Actions</h3>

      {{!-- TODO: List land use actions --}}
