<h2>new project</h2>
<SaveableForm
    @model={{@package}}
    @validators={{array (hash) this.validations.SubmittablePackageFormValidations}}
    as | saveablePackageForm |
>
        <saveablePackageForm.SaveableForm
            @model={{saveablePackageForm.data.projectForm}}
            @validators={{}}
        >
            {{log "project form" saveablePackageForm.data}}
            <div class="grid-x grid-margin-x">
                <div class="cell large-8"></div>
                <div class="cell large-4 sticky-sidebar">
                    <button>Submit</button>
                </div>
            </div>
        </saveablePackageForm.SaveableForm>
</SaveableForm>
