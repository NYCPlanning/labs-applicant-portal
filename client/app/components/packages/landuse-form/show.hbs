<div class="grid-x grid-margin-x">
  <div class="cell large-8">

    <section class="form-section">
      <h1 class="header-large">
        <span id="project-info" class="section-anchor"></span>
        Land Use Information Confirmation
        <small class="text-weight-normal">
          {{if @package.dcpPackageversion (concat '(V' @package.dcpPackageversion ')')}}
        </small>
      </h1>

      <h2 class="no-margin" data-test-show="dcpProjectname">
        {{@package.project.dcpProjectname}}
        <small class="text-weight-normal">
          {{if @package.project.dcpName (concat '(' @package.project.dcpName ')')}}
        </small>
      </h2>

      <p class="text-large text-dark-gray">
        {{optionset 'bbl' 'boroughs' 'label' @package.project.dcpBorough}} |
        {{optionset 'package' 'statuscode' 'label' @package.statuscode}}
      </p>
    </section>


    <section class="form-section">
      <h2 class="section-header">
        <span id="project-description" class="section-anchor"></span>
        Project Description
      </h2>

      <Ui::Answer @beside={{true}} as |A|>
        <A.Prompt>
          Name
        </A.Prompt>
        <A.Field>
          {{@package.landuseForm.dcpContactname}}
        </A.Field>
      </Ui::Answer>

      <Ui::Answer @beside={{true}} as |A|>
        <A.Prompt>
          Phone
        </A.Prompt>
        <A.Field>
          {{@package.landuseForm.dcpContactphone}}
        </A.Field>
      </Ui::Answer>

      <Ui::Answer @beside={{true}} as |A|>
        <A.Prompt>
          Email
        </A.Prompt>
        <A.Field>
          {{@package.landuseForm.dcpContactemail}}
        </A.Field>
      </Ui::Answer>

    </section>

    <section class="form-section">
      <h2 class="section-header">
        <span id="site-information" class="section-anchor"></span>
        Site Information
      </h2>

      <Ui::Answer @beside={{true}} as |A|>
        <A.Prompt>
          Primary Street Address
        </A.Prompt>
        <A.Field>
          {{@package.landuseForm.dcpSitedataadress}}
        </A.Field>
      </Ui::Answer>

      <Ui::Answer @beside={{true}} as |A|>
        <A.Prompt>
          City Council District(s)
        </A.Prompt>
        <A.Field>
          {{@package.landuseForm.dcpCitycouncil}}
        </A.Field>
      </Ui::Answer>

      <Ui::Answer @beside={{true}} as |A|>
        <A.Prompt>
          Community District(s)
        </A.Prompt>
        <A.Field>
          {{@package.landuseForm.dcpSitedatacommunitydistrict}}
        </A.Field>
      </Ui::Answer>

      <Ui::Answer @beside={{true}} as |A|>
        <A.Prompt>
          Zoning Sectional Map Number(s)
        </A.Prompt>
        <A.Field>
          {{@package.landuseForm.dcpSitedatazoningsectionnumbers}}
        </A.Field>
      </Ui::Answer>

      <Ui::Answer @beside={{true}} as |A|>
        <A.Prompt>
          Existing Zoning District(s)
        </A.Prompt>
        <A.Field>
          {{@package.landuseForm.dcpSitedataexistingzoningdistrict}}
        </A.Field>
      </Ui::Answer>

      <Ui::Answer @beside={{true}} as |A|>
        <A.Prompt>
          Special District(s)
        </A.Prompt>
        <A.Field>
          {{@package.landuseForm.dcpSpecialdistricts}}
        </A.Field>
      </Ui::Answer>

    </section>


    <section class="form-section">
      <h2 class="section-header">
        <span id="applicant-team" class="section-anchor"></span>
        Applicant Team
      </h2>

      <ul class="no-bullet">
        {{#each @package.landuseForm.applicants as |applicant|}}
          <li class="ruled-adjacent">
            <h4 class="no-margin">
              {{#if applicant.dcpOrganization}}
                {{applicant.dcpOrganization}} | {{optionset 'applicant' 'dcpType' 'label' applicant.dcpType}} |
              {{/if}}
              {{applicant.dcpFirstname}} {{applicant.dcpLastname}}
            </h4>
            {{#if applicant.dcpEmail}}
              <div>
                <FaIcon @icon="envelope" class="text-gray" @fixedWidth={{true}} />
                {{applicant.dcpEmail}}
              </div>
            {{/if}}
            {{#if applicant.dcpAddress}}
              <div>
                <FaIcon @icon="map-marker-alt" class="text-gray" @fixedWidth={{true}} />
                {{applicant.dcpAddress}}, {{applicant.dcpCity}},
                {{optionset 'applicant' 'dcpState' 'label' applicant.dcpState}} {{applicant.dcpZipcode}}
              </div>
            {{/if}}
            {{#if applicant.dcpPhone}}
              <div>
                <FaIcon @icon="phone-square" class="text-gray" @fixedWidth={{true}} />
                {{applicant.dcpPhone}}
              </div>
            {{/if}}
          </li>
        {{/each}}
      </ul>
    </section>

    <section class="form-section">
      <h2 class="section-header">
        <span id="project-area" class="section-anchor"></span>
        Proposed Project Area
      </h2>
      <p>
        The <b>Project Area</b> is the entirety of all land parcels
        that are affected by all actions being sought.
      </p>

      <Ui::Answer @beside={{true}} as |A|>
        <A.Prompt>
          Do <em>all</em> Actions being sought apply to the whole City?
        </A.Prompt>
        <A.Field>
          {{optionset 'landuseForm' 'dcpWholecity' 'label' @package.landuseForm.dcpWholecity}}
        </A.Field>
      </Ui::Answer>

      {{#if (eq
            @package.landuseForm.dcpWholecity
            (optionset 'landuseForm' 'dcpWholecity' 'code' 'NO')
          )}}

        <Ui::Answer @beside={{true}} as |A|>
          <A.Prompt>
            Do <em>all</em> Actions being sought apply to the entirety of one or
            more Boroughs?
          </A.Prompt>
          <A.Field>
            {{optionset 'landuseForm' 'dcpEntiretyboroughs' 'label' @package.landuseForm.dcpEntiretyboroughs}}
          </A.Field>
        </Ui::Answer>
        {{#if (eq
              @package.landuseForm.dcpEntiretyboroughs
              (optionset 'landuseForm' 'dcpEntiretyboroughs' 'code' 'YES')
            )}}
          <Ui::Answer @beside={{true}} as |A|>
            <A.Prompt>
              Which Boroughs?
            </A.Prompt>
            <A.Field>
              {{@package.landuseForm.dcpBoroughs}}
            </A.Field>
          </Ui::Answer>
        {{/if}}
        {{#if (eq
              @package.landuseForm.dcpEntiretyboroughs
              (optionset 'landuseForm' 'dcpEntiretyboroughs' 'code' 'NO')
            )}}
          <Ui::Answer @beside={{true}} as |A|>
            <A.Prompt>
              Do <em>all</em> Actions being sought apply to the entirety of one or
              more Community District(s)?
            </A.Prompt>
            <A.Field>
              {{optionset 'landuseForm' 'dcpEntiretycommunity' 'label' @package.landuseForm.dcpEntiretycommunity}}
            </A.Field>
          </Ui::Answer>
          {{#if (eq
                @package.landuseForm.dcpEntiretycommunity
                (optionset 'landuseForm' 'dcpEntiretycommunity' 'code' 'YES')
              )}}
            <Ui::Answer @beside={{true}} as |A|>
              <A.Prompt>
                Which Community District(s)?
              </A.Prompt>
              <A.Field>
                {{@package.landuseForm.dcpCommunity}}
              </A.Field>
            </Ui::Answer>
          {{/if}}
          {{#if (eq
                @package.landuseForm.dcpEntiretycommunity
                (optionset 'landuseForm' 'dcpEntiretycommunity' 'code' 'NO')
              )}}
            <Ui::Answer @beside={{true}} as |A|>
              <A.Prompt>
                Do <em>all</em> Actions being sought apply to
                land or land underwater that is not associated
                with a Tax Block or Lot?
              </A.Prompt>
              <A.Field>
                {{optionset 'landuseForm' 'dcpNotaxblock' 'label' @package.landuseForm.dcpNotaxblock}}
              </A.Field>
            </Ui::Answer>
            {{#if (eq
                @package.landuseForm.dcpNotaxblock
                (optionset 'landuseForm' 'dcpNotaxblock' 'code' 'YES')
              )}}
              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Describe Non-Tax Lots by cross streets
                  bounding streets, dimensions, etc.
                </A.Prompt>
                <A.Field>
                  {{@package.landuseForm.dcpSitedatapropertydescription}}
                </A.Field>
              </Ui::Answer>
            {{/if}}
          {{/if}}
        {{/if}}
      {{/if}}

      {{#if (and
        (eq @package.landuseForm.dcpWholecity (optionset 'landuseForm' 'dcpWholecity' 'code' 'NO'))
        (eq @package.landuseForm.dcpEntiretyboroughs (optionset 'landuseForm' 'dcpEntiretyboroughs' 'code' 'NO'))
        (eq @package.landuseForm.dcpEntiretycommunity (optionset 'landuseForm' 'dcpEntiretycommunity' 'code' 'NO'))
        (eq @package.landuseForm.dcpNotaxblock (optionset 'landuseForm' 'dcpNotaxblock' 'code' 'NO'))
      )}}
        <Ui::Answer @beside={{true}} as |A|>
          <A.Prompt>
            What are the existing Zoning Districts and Special Districts?
          </A.Prompt>
          <A.Field>
            {{@package.landuseForm.dcpZonesspecialdistricts}}
          </A.Field>
        </Ui::Answer>

        <Ui::Answer @beside={{true}} as |A|>
          <A.Prompt>
            Is any portion of the proposed Project Area located
            within the State Designated Coastal Zone Management
            (CZM) Area?
          </A.Prompt>
          <A.Field>
            {{optionset 'landuseForm' 'dcpStateczm' 'label' @package.landuseForm.dcpStateczm}}
          </A.Field>
        </Ui::Answer>

        <Ui::Answer @beside={{true}} as |A|>
          <A.Prompt>
            Is any portion of the Proposed Project Area located within a Historic District?
          </A.Prompt>
          <A.Field>
            {{optionset 'landuseForm' 'dcpHistoricdistrict' 'label' @package.landuseForm.dcpHistoricdistrict}}
          </A.Field>
        </Ui::Answer>

        {{#if (eq
                @package.landuseForm.dcpHistoricdistrict
                (optionset 'landuseForm' 'dcpHistoricdistrict' 'code' 'YES')
              )}}
          <Ui::Answer @beside={{true}} as |A|>
            <A.Prompt>
              Historic District Name
            </A.Prompt>
            <A.Field>
              {{@package.landuseForm.dcpSitedatarenewalarea}}
            </A.Field>
          </Ui::Answer>
        {{/if}}
      {{/if}}
    </section>

    {{#if (and
      (eq @package.landuseForm.dcpWholecity (optionset 'landuseForm' 'dcpWholecity' 'code' 'NO'))
      (eq @package.landuseForm.dcpEntiretyboroughs (optionset 'landuseForm' 'dcpEntiretyboroughs' 'code' 'NO'))
      (eq @package.landuseForm.dcpEntiretycommunity (optionset 'landuseForm' 'dcpEntiretycommunity' 'code' 'NO'))
      (eq @package.landuseForm.dcpNotaxblock (optionset 'landuseForm' 'dcpNotaxblock' 'code' 'NO'))
    )}}
      <section class="form-section">
        <h2 class="section-header">
          <span id="proposed-development-site" class="section-anchor"></span>
          Proposed Development Site
        </h2>
        <p>
          The <b>Development Site</b> is the specific parcel(s) that the Applicant is seeking to develop.
          A Project Area and Development Site can be the same parcels of land or different,
          depending on the Actions being sought. For instance, a Special District may be mapped
          over a portion of a neighborhood (Project Area), but only certain parcels within it may
          be proposed for immediate development by the Applicant (Development Site).
        </p>
        
        <Ui::Answer @beside={{true}} as |A|>
          <A.Prompt>
            Does the Application result in the development of 500,000+ ZSF of floor area?
          </A.Prompt>
          <A.Field>
            {{optionset 'landuseForm' 'dcp500kpluszone' 'label' @package.landuseForm.dcp500kpluszone}}
          </A.Field>
        </Ui::Answer>

        {{#if (eq
              @package.landuseForm.dcp500kpluszone
              (optionset 'landuseForm' 'dcp500kpluszone' 'code' 'YES')
            )}}
          <Ui::Answer @beside={{true}} as |A|>
            <A.Prompt>
              Development size
            </A.Prompt>
            <A.Field>
              {{optionset 'landuseForm' 'dcpDevsize' 'label' @package.landuseForm.dcpDevsize}}
            </A.Field>
          </Ui::Answer>
        {{/if}}

        <Ui::Answer @beside={{true}} as |A|>
          <A.Prompt>
            Is the Development Site a Landmark (New York City or other) or within a Historic District?
          </A.Prompt>
          <A.Field>
            {{optionset 'landuseForm' 'dcpSitedatasiteisinnewyorkcity' 'label' @package.landuseForm.dcpSitedatasiteisinnewyorkcity}}
          </A.Field>
        </Ui::Answer>

        {{#if (eq
              @package.landuseForm.dcpSitedatasiteisinnewyorkcity
              (optionset 'landuseForm' 'dcpSitedatasiteisinnewyorkcity' 'code' 'Yes')
            )}} 
          <Ui::Answer @beside={{true}} as |A|>
            <A.Prompt>
              Landmark or Historic District name
            </A.Prompt>
            <A.Field>
              {{@package.landuseForm.dcpSitedataidentifylandmark}}
            </A.Field>
          </Ui::Answer>
        {{/if}}
      </section>

      <section class="form-section">
        <h2 class="section-header">
          <span id="project-area-tax-lots" class="section-anchor"></span>
          Project Area Tax Lots
        </h2>
        <p>Tax Lots Included in This Project:</p>
        <ul class="no-bullet">
          {{#each @package.landuseForm.bbls as |bbl|}}
          <li class="ruled-adjacent">
            <fieldset class="fieldset">
              <h3 class="small-margin-bottom">
                {{bbl.dcpBblnumber}}
              </h3>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Is this BBL part of the Development Site?
                </A.Prompt>
                <A.Field>
                  {{optionset 'bbl' 'dcpDevelopmentsite' 'label' bbl.dcpDevelopmentsite}}
                </A.Field>
              </Ui::Answer>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Is only a portion of this lot included in the project area?
                </A.Prompt>
                <A.Field>
                  {{optionset 'bbl' 'dcpPartiallot' 'label' bbl.dcpPartiallot}}
                </A.Field>
              </Ui::Answer>

            </fieldset>
          </li>
          {{/each}}
        </ul>

      </section>

    {{/if}}





    <section class="form-section">
      <h2 class="section-header">
        <span id="environmental-review" class="section-anchor"></span>
        Environmental Review
      </h2>

      <Ui::Answer @beside={{true}} as |A|>
        <A.Prompt>
          Who is the lead agency?
        </A.Prompt>
        <A.Field>
          {{@package.landuseForm.leadAgency.name}}
        </A.Field>
      </Ui::Answer>

      <Ui::Answer @beside={{true}} as |A|>
        <A.Prompt>
          What is the CEQR number?
        </A.Prompt>
        <A.Field>
          {{@package.landuseForm.dcpCeqrnumber}}
        </A.Field>
      </Ui::Answer>

      <Ui::Answer @beside={{true}} as |A|>
        <A.Prompt>
          What is the CEQR type?
        </A.Prompt>
        <A.Field>
          {{optionset 'landuseForm' 'dcpCeqrtype' 'label' @package.landuseForm.dcpCeqrtype}}
        </A.Field>
      </Ui::Answer>

      <Ui::Answer @beside={{true}} as |A|>
        <A.Prompt>
          What is the Type II Category?
        </A.Prompt>
        <A.Field>
          {{@package.landuseForm.dcpTypecategory}}
        </A.Field>
      </Ui::Answer>

    </section>

    <section class="form-section">
      <h2 class="section-header">
        <span id="proposed-actions" class="section-anchor"></span>
        Proposed Actions
      </h2>
      <div data-test-section="proposed-actions">
        {{#if this.projectHasRequiredActions}}
          <h3 class="large-margin-top">Zoning Special Permits, Authorizations, and Certifications</h3>
          <p>The following questions relate to Zoning Special Permits, Authorizations and Certifications and the following
            Actions: [ZA, ZR].</p>

          <Ui::Answer as |A|>
            <A.Prompt>
              The applicant is any of the following:
            </A.Prompt>
            <A.Field>
              <span data-test-applicantPropertyTypeList>
                <Packages::LanduseForm::ApplicantPropertyTypeList @landuseForm={{@package.landuseForm}} />
              </span>
            </A.Field>
          </Ui::Answer>

          <Ui::Answer @beside={{true}} as |A|>
            <A.Prompt>
              Are there other owners or long-term lessees of the subject property?
            </A.Prompt>
            <A.Field>
              {{optionset 'landuseForm' 'dcpOtherparties' 'label' @package.landuseForm.dcpOtherparties}}
            </A.Field>
          </Ui::Answer>
        {{/if}}

        <h3 class="large-margin-top">All Proposed Project Actions</h3>
        
        <Ui::Answer @beside={{true}} as |A|>
          <A.Prompt>
            Does this Project require a Legal Instrument to be recorded against the subject property?
          </A.Prompt>
          <A.Field>
            {{optionset 'landuseForm' 'dcpLegalinstrument' 'label' @package.landuseForm.dcpLegalinstrument}}
          </A.Field>
        </Ui::Answer>

        {{#each @package.landuseForm.landuseActions as |landuseAction|}}
          <fieldset class="fieldset">

            {{#if landuseAction.dcpDatadifferentiationmultipleactionsametype}}
              <p>{{landuseAction.dcpDatadifferentiationmultipleactionsametype}}. This information differentiates multiple
                Actions with the same Action Code.</p>
            {{/if}}

            <Ui::Legend>
              <span class="label">
                {{#if (lookup-action-type landuseAction.dcpActioncode)}}
                  <LabsUi::IconTooltip @tip={{lookup-action-type landuseAction.dcpActioncode}} @icon="info-circle"
                    @side="bottom" @transform="shrink-4 up-2" @fixedWidth={{true}} />
                {{/if}}
                {{~landuseAction.dcpActioncode~}}
              </span>
            </Ui::Legend>

            <Ui::Answer @beside={{true}} as |A|>
              <A.Prompt>
                Is this an action where the applicant is solely a public agency?
              </A.Prompt>
              <A.Field>
                {{optionset 'landuseAction' 'dcpApplicantispublicagencyactions' 'label' landuseAction.dcpApplicantispublicagencyactions}}
              </A.Field>
            </Ui::Answer>

            {{#if this.projectHasRequiredActions}}
              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Zoning Resolution section name
                </A.Prompt>
                <A.Field>
                  {{landuseAction.dcpNameofzoningresolutionsectionQ.questionId}}
                </A.Field>
              </Ui::Answer>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Zoning Resolution section(s) to be modified
                </A.Prompt>
                <A.Field>
                  {{landuseAction.dcpZoningresolutionsectionactionispursuant}}
                </A.Field>
              </Ui::Answer>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Number of Zoning Lot(s) affected
                </A.Prompt>
                <A.Field>
                  {{landuseAction.dcpNumberofzoninglotsaffected}}
                </A.Field>
              </Ui::Answer>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Square footage of Zoning Lot(s) affected
                </A.Prompt>
                <A.Field>
                  {{landuseAction.dcpSquarefootageofzoninglotsaffected}}
                </A.Field>
              </Ui::Answer>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Square footage of the Proposed Development
                </A.Prompt>
                <A.Field>
                  {{landuseAction.dcpSquarefootageoftheproposeddevelopment}}
                </A.Field>
              </Ui::Answer>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Square footage associated with transfer of Development Rights or Floor Area Bonus
                </A.Prompt>
                <A.Field>
                  {{landuseAction.dcpSquarefootassociatedwithtransferbonus}}
                </A.Field>
              </Ui::Answer>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Proposed number of dwelling units
                </A.Prompt>
                <A.Field>
                  {{landuseAction.dcpNumberofdu}}
                </A.Field>
              </Ui::Answer>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Is the Action to authorize or permit an open use?
                </A.Prompt>
                <A.Field>
                  {{landuseAction.dcpistheactiontoauthorizeorpermitanopenuse}}
                </A.Field>
              </Ui::Answer>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Is the Action to authorize commercial or community facility use?
                </A.Prompt>
                <A.Field>
                  {{landuseAction.dcpIstheactiontoauthorizeacommercial}}
                </A.Field>
              </Ui::Answer>

            {{/if}}

            <Ui::Answer @beside={{true}} as |A|>
              <A.Prompt>
                This is a follow-up Action. The previous approval Action Code:
              </A.Prompt>
              <A.Field>
                {{optionset 'landuseAction' 'actions' 'label' landuseAction.dcpPreviouslyapprovedactioncode}}
              </A.Field>
            </Ui::Answer>

            {{#if this.projectHasRequiredActionsAndFollowUpYes}}

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Date of previous approval
                </A.Prompt>
                <A.Field>
                  {{landuseAction.dcpDateofpreviousapproval}}
                </A.Field>
              </Ui::Answer>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Lapse date of previous approval
                </A.Prompt>
                <A.Field>
                  {{landuseAction.dcpLapsedateofpreviousapproval}}
                </A.Field>
              </Ui::Answer>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Is this a modification subject to Section 197-c of the City Charter?
                </A.Prompt>
                <A.Field>
                  {{optionset 'landuseAction' 'dcpModsubjectto197c' 'label' landuseAction.dcpModsubjectto197c}}
                </A.Field>
              </Ui::Answer>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Is the Action a modification to or pursuant to a Legal Instrument?
                </A.Prompt>
                <A.Field>
                  {{optionset 'landuseAction' 'dcpIndicatewhetheractionisamodification' 'label' landuseAction.dcpDateofpreviousapproval}}
                </A.Field>
              </Ui::Answer>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Type of Legal Instrument
                </A.Prompt>
                <A.Field>
                  {{landuseAction.dcpTypeoflegalinstrument}}
                </A.Field>
              </Ui::Answer>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  CRFN or other tracking number for Legal Instrument
                </A.Prompt>
                <A.Field>
                  {{landuseAction.dcpCrfnnumber}}
                </A.Field>
              </Ui::Answer>

              <Ui::Answer @beside={{true}} as |A|>
                <A.Prompt>
                  Recordation date of Legal Instrument
                </A.Prompt>
                <A.Field>
                  {{landuseAction.dcpRecordationdate}}
                </A.Field>
              </Ui::Answer>

            {{/if}}
          </fieldset>
        {{/each}}
      </div>
    </section>


    <section class="form-section">
      <h2 class="section-header">
        <span id="related-actions" class="section-anchor"></span>
        Related Actions
      </h2>
      <ul class="no-bullet">
        {{#each @package.landuseForm.relatedActions as |relatedAction|}}
          <li class="ruled-adjacent">
            <h4 class="no-margin">
              Related Action
            </h4>
            <div>
              {{#if relatedAction.dcpDevelopmentsite}}
              {{optionset 'landuseForm' 'dcpDevelopmentsite' 'label' @package.landuseForm.dcpDevelopmentsite}}
              {{/if}}
            </div>
            <div>
              Is the action completed:
              {{optionset 'relatedAction' 'dcpIscompletedaction' 'label' relatedAction.dcpIscompletedaction}}
            </div>
            <div>
              {{#if relatedAction.dcpReferenceapplicationno}}
                Reference/Applicant Number: {{relatedAction.dcpReferenceapplicationno}}
              {{/if}}
            </div>

            <div>
              {{#if relatedAction.dcpApplicationdescription}}
               Description: {{relatedAction.dcpApplicationdescription}}
              {{/if}}
            </div>

            <div>
